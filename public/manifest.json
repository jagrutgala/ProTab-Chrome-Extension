{
    "manifest_version": 3,
    "name": "Pro Tab",
    "description": "Default new tab replacement",
    "version": "1.0",
    "permissions": [
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "ProTab Ext"
    },
    "options_page": "options.html"
}